;; safe division
; zähler speichen
dld 100
sto 1
; nenner speichern
dld 10
sto 2
; fehlerwert speichen
dld -1
sto 3
; bei nenner = 0 zu fehlerbehandlung springen
ld 2
jeq 11
ld 1
; division, ergebnis in R3 speichern
div 2
sto 3
; R3 laden , fehlerbehandlung: R3 enthält -1 bei sprung 
; ansonsten das ergebnis
ld 3
end 1
